@using DAF.Core;
@using DAF.Web;
@using DAF.CMS;
@using DAF.CMS.Models;
@{
    Dictionary<string, string> paras = CmsHelper.GetControlParas(Page.Paras);
    IEnumerable<Content> objs  = CmsHelper.GetContents(paras["cate"], Convert.ToInt32(paras["pi"]), Convert.ToInt32(paras["ps"]));
}
<table class="table table-bordered">
    <thead>
        <tr>
            <th></th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var obj in objs)
        {
            <tr>
                <td>@obj.Title</td>
                <td></td>
            </tr>
        }
    </tbody>
</table>
