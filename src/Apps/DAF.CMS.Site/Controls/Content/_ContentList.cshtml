[ 
    { Name:'cate', DefaultValue:'', Description:'栏目编码，一般取自query string，可输入：cmd(qs name:cate)。' }
    ,{ Name:'pi', DefaultValue:'0', Description:'当前页码，一般取自query string，可输入：cmd(qs name:pi)。' }
    ,{ Name:'ps', DefaultValue:'20', Description:'每页页数，一般取自query string，可输入：cmd(qs name:ps)。' }
