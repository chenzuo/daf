@using DAF.Core;
@using DAF.Web;
@using DAF.CMS;
@using DAF.CMS.Models;
@{
    IEnumerable<Content> objs = Page.Data as IEnumerable<Content>;
    if(objs == null) {
        
        objs = CmsHelper.GetContents(Page.Category, Page.PageIndex ?? 0, Page.PageSize ?? 20, Page.Language);
    }
}
<div class="content-list @Page.Css">
    <ul>
        @foreach (var obj in objs)
        {
        <li><a href="">@obj.Title</a></li>
        }
    </ul>
</div>
